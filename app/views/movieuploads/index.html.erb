<p id="notice"><%= notice %></p>

<h1>Movieuploads</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Rating</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movieuploads.each do |movieupload| %>
      <tr>
        <td><%= movieupload.name %></td>
        <td><%= movieupload.rating %></td>
        <td><%= movieupload.description %></td>
        <td><%= link_to 'Show', movieupload %></td>
        <td><%= link_to 'Edit', edit_movieupload_path(movieupload) %></td>
        <td><%= link_to 'Destroy', movieupload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movieupload', new_movieupload_path %>
